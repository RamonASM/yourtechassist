{
  "name": "yourtechassist",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:marketing": "turbo run dev --filter=@yta/marketing",
    "dev:portal": "turbo run dev --filter=@yta/client-portal",
    "dev:api": "turbo run dev --filter=@yta/api",
    "build": "turbo run build",
    "build:marketing": "turbo run build --filter=@yta/marketing",
    "build:portal": "turbo run build --filter=@yta/client-portal",
    "build:api": "turbo run build --filter=@yta/api",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "turbo run db:generate --filter=@yta/api",
    "db:push": "turbo run db:push --filter=@yta/api",
    "db:migrate": "turbo run db:migrate --filter=@yta/api",
    "db:seed": "turbo run db:seed --filter=@yta/api"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
