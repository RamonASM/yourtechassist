{
  "name": "yourtechassist",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "pnpm --parallel -r run dev",
    "build": "pnpm -r run build",
    "build:marketing": "pnpm --filter @yta/marketing run build",
    "build:portal": "pnpm --filter @yta/client-portal run build",
    "build:api": "pnpm --filter @yta/api run build",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
